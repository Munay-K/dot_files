	<!-- General information -->
	<!-- Title -->
	<title>Title</title>
	<!-- Character set used to display the content of the website-->
	<meta charset="UTF-8>
	
	<!-- Summarize of the content of the page -->
	<!-- Description of website for web browsers-->
	<meta name="description" content="General and brief description of the website.">

	<!-- Define keywords of our own page. -->
	<meta name="keywords" content="keywords">

	<!-- Define the author of a page. -->
	<meta name="author" content="Author name">
	<!-- Instructions on how to control the page's dimension and scaling -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	
	<!-- Dynammic styles -->
	<!-- CSS Styles -->
	<link rel="stylesheet" type="text/css" href="">
	<!-- JS files -->
	<script src=""></script>
